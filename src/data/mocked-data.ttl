@prefix : <#>.
@prefix todo: <https://icanhasweb.net/vocab/todo.ttl#>.
@prefix meg: </>.

:defaultList
    a todo:TodoList;
    todo:hasTask :task-1, :task-2;
    todo:listName "Todo list";
    meg:taskDone "true".

:task-1 a todo:TodoTask; todo:taskDescription "Foo"; todo:taskDone false.

:task-2 a todo:TodoTask; todo:taskDescription "Bar"; todo:taskDone false.

