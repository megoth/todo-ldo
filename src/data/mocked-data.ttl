@prefix todo: <https://icanhasweb.net/vocab/todo.ttl#> .

<#defaultList>
    a             todo:TodoList ;
    todo:listName "Todo list A" ;
    todo:hasTask  <#task-1>, <#task-2> .

<#task-1>
    a                    todo:TodoTask ;
    todo:taskDescription "Foo" .

<#task-2>
    a                    todo:TodoTask ;
    todo:taskDescription "Bar" ;
    todo:taskDone        true .
